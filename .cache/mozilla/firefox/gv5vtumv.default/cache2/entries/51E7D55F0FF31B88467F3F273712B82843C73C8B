			<script type="text/javascript" charset="utf-8">
E.domReady(function () {
    page.repaymentinfo.init();

    E.connect($("J_btnSubmit"), "click", function (e) {

        var itemArea = $("J_checkAgreeItem");
        var chkInput = itemArea.query("input")[0];
        var iptExplain = itemArea.query(".ui-form-explain")[0];


        var submitItem = $("J_btnSubmit");

        var btnSubmit = submitItem.query('input[type="submit"]')[0];

        if (chkInput.node.checked != true) {

            itemArea.addClass("ui-form-error");
            iptExplain.setHtml("请同意支付宝还款协议");
            e.preventDefault();
            return false;
        } else {
            itemArea.removeClass("ui-form-error");
            iptExplain.setHtml("");


            //disable


            btnSubmit.attr("disabled", "disabled");

            submitItem.addClass("ui-button-disable");

            window.location.href = $("paymentForm").attr('action');
            return false;
        }


    });

    seajs.use(['$'], function($) {
        $('.button-center .link-modify').on('click', function (e) {
            e.preventDefault();
            $('#paymentForm').attr('action', $(this).attr('href')).submit();
        });
    });
});
</script>
		
	 			

		
		








                                                                                                                                                                                                                                        
