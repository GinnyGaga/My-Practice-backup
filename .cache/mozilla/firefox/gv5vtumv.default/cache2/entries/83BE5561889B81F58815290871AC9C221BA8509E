seajs.use(['$'], function($) {
    var cdbrepayquery = {
        init: function() {
            this.initDeleteTool('#userid');
            this.bindEvent();
        },
        initDeleteTool: function(selector) {
            var ele = $(selector);
            ele.val() && ele.after('<span class="ui-tip"><i class="input-delete" title="É¾³ý">¡Á</i></span>');

            var del = ele.next().find('.input-delete');
            del.on('click', function(){
                ele.val('').trigger('change').focus();
                $(this).hide();
            });
        },
        bindEvent: function() {
            this.changeValue();
        },
        changeValue: function() {
            var maskedCertNo = $('#maskedCertNo');
            var cerNoInput = $('#passportCard');

            $('#userid').change(function(){
                if(!cerNoInput.is(":visible")) {
                    maskedCertNo.remove();
                    cerNoInput.show().val('').next('.ui-form-explain').show();
                }
            });
        }
    };

    cdbrepayquery.init();
});

</script>
		
	 			

		
		








                                                                                                                                                                                                                                        
